{
    "name": "woohoolabs/harmony",
    "description": "Woohoo Labs. Harmony",
    "type": "library",
    "keywords": ["Woohoo Labs.", "Harmony", "Framework", "PSR-7", "PSR-11"],
    "homepage": "http://harmony.woohoolabs.com",
    "license": "MIT",
    "authors": [
        {
            "name":"Máté Kocsis",
            "email":"kocsismate@woohoolabs.com"
        }
    ],
    "support": {
        "issues": "https://github.com/woohoolabs/harmony/issues",
        "source": "https://github.com/woohoolabs/harmony"
    },
    "require": {
        "php":"^7.0.0",
        "psr/container": "^1.0.0@dev",
        "psr/http-message-implementation": "^1.0.0"
    },
    "require-dev": {
        "nikic/fast-route": "^1.0.0",
        "phpunit/phpunit": "^5.4.0",
        "squizlabs/php_codesniffer": "^2.1.1",
        "zendframework/zend-diactoros": "^1.3.0"
    },
    "suggest": {
        "nikic/fast-route": "Allows to use the default router"
    },
    "provide": {
        "psr/container-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "WoohooLabs\\Harmony\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WoohooLabsTest\\Harmony\\": "tests/",
            "WoohooLabs\\Harmony\\Examples\\": "examples/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
